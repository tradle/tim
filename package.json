{
  "name": "Tradle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "./scripts/start.sh",
    "start:resetcache": "RESET_CACHE=1 ./scripts/start.sh",
    "start:ios": "PLATFORM=ios ./scripts/start.sh",
    "start:android": "PLATFORM=android ./scripts/start.sh",
    "start:chrome": "&& node --max_old_space_size=4096 ./node_modules/.bin/react-native start",
    "postinstall": "./scripts/postinstall.sh",
    "postinstal": "npm run postinstall",
    "postintall": "npm run postinstall",
    "postintsal": "npm run postinstall",
    "dedupe-deps": "DEBUG=dedupe-deps dedupe-deps --dir .",
    "nodeify": "./node_modules/.bin/rn-nodeify --hack --exclude net,tls",
    "installhooks": "cp hooks/* .git/hooks/",
    "rmsymlinks": "find -L . -type l -exec rm {} \\;",
    "reset": "rm -fr $TMPDIR/react-* && watchman watch-del-all",
    "lint": "eslint --quiet .",
    "lintfile": "eslint --quiet",
    "precommit": "npm run lint",
    "dev:setup": "cd iOS && bundle install && cd ../android && bundle install && npm run loadsecrets",
    "bundle:ios": "./bundle.sh ios",
    "bundle:android": "./bundle.sh android",
    "ios:run": "./scripts/run-on-device.sh ios $@",
    "ios:testflight": "cd iOS && fastlane ios --env staging beta_testflight $@",
    "ios:fabric": "cd iOS && fastlane ios --env staging beta_fabric $@",
    "ios:release": "cd iOS && fastlane ios release $@",
    "android:run": "./scripts/run-on-device.sh android $@",
    "android:fabric": "cd iOS && bundle exec fastlane android beta $@",
    "android:dryrun": "cd iOS && bundle exec fastlane android release_dryrun $@",
    "android:release": "cd iOS && bundle exec fastlane android release $@",
    "loadsecrets": "./scripts/placesecrets.sh",
    "clean:node_modules": "./scripts/clean_node_modules.js",
    "update:models": "npm i -S tradle/models tradle/custom-models tradle/models-corporate-onboarding pgmemk/models-nz tradle/models-onfido tradle/models-products-bot && ./reshrink.sh",
    "push:ios:staging": "./scripts/code-push.js --platform ios --stage Staging",
    "push:ios:prod": "./scripts/code-push.js --platform ios --stage Production",
    "push:android:staging": "./scripts/code-push.js --platform android --stage Staging",
    "push:android:prod": "./scripts/code-push.js --platform android --stage Production",
    "fix:staging": "node ./scripts/fix-staging.js",
    "build:codepush:android": "./scripts/release-android.sh",
    "build:codepush:ios": "echo 'ERROR: PLEASE BUILD IN XCODE FOR NOW'",
    "pushenv": "./scripts/push-environment.sh",
    "diffenv": "./scripts/diff-environment.sh",
    "translate:strings": "node ./scripts/translateStrings -d ./utils",
    "translate:xmlstrings": "node ./scripts/translateStrings -d ./android/app/src/main/res/values -l vi,bn,fil,es,fr,nl,ru,km -f",
    "android-dev": "adb reverse tcp:8081 tcp:8081 && react-native run-android",
    "nibble": "eslint-nibble Components/ Store/ utils/ index.common.js"
  },
