{
  "name": "Tradle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node web/server",
    "dev": "NODE_ENV=development npm start",
    "prod": "NODE_ENV=production npm start",
    "postinstall": "npm run installhooks && ./web.sh",
    "installhooks": "cp hooks/* .git/hooks/",
    "clean": "rimraf web/dist/* ",
    "copyassets": "shopt -s dotglob && cp -RL web/public/* web/dist && shopt -u dotglob",
    "analyze": "npm run build -- --json | node web/fix-analysis.js > web/dist/analysis.json && webpack-bundle-size-analyzer web/dist/analysis.json",
    "build": "mkdir -p web/dist && npm run clean && NODE_ENV=production node --max_old_space_size=4096 $(which webpack) --config web/webpack.config.js --progress --colors && npm run copyassets",
    "build:unminified": "mkdir -p web/dist && npm run clean && NODE_ENV=development node --max_old_space_size=4096 $(which webpack) --config web/webpack.config.js --progress --colors && npm run copyassets",
    "build:dev": "PROVIDERS=dev npm run build",
    "build:a": "PROVIDERS=a npm run build",
    "build:a2": "PROVIDERS=a2 npm run build",
    "build:ae": "PROVIDERS=ae npm run build",
    "build:ae2": "PROVIDERS=ae2 npm run build",
    "build:l": "PROVIDERS=l npm run build",
    "build:u": "PROVIDERS=u npm run build",
    "build:s": "PROVIDERS=s npm run build",
    "build:static": "PROVIDERS=static npm run build",
    "build:local": "PROVIDERS=local npm run build",
    "build:bots": "PROVIDERS=bots npm run build",
    "deploy": "GIT_DEPLOY_DIR=web/dist GIT_DEPLOY_BRANCH=gh-pages GIT_DEPLOY_REPO=origin ./deploy.sh",
    "deploy:pgmemk": "npm run build && GIT_DEPLOY_DIR=web/dist GIT_DEPLOY_BRANCH=gh-pages GIT_DEPLOY_REPO=origin ./deploy.sh",
    "deploy:dev": "npm run build:dev && GIT_DEPLOY_DIR=web/dist GIT_DEPLOY_BRANCH=gh-pages GIT_DEPLOY_REPO=mv ./deploy.sh",
    "deploy:a": "npm run build:a && GIT_DEPLOY_DIR=web/dist GIT_DEPLOY_BRANCH=gh-pages-a GIT_DEPLOY_REPO=mv ./deploy.sh",
    "deploy:a2": "npm run build:a2 && GIT_DEPLOY_DIR=web GIT_DEPLOY_BRANCH=gh-pages-a2 GIT_DEPLOY_REPO=mv ./deploy.sh",
    "deploy:ae": "npm run build:ae && GIT_DEPLOY_DIR=web/dist GIT_DEPLOY_BRANCH=gh-pages-ae GIT_DEPLOY_REPO=mv ./deploy.sh",
    "deploy:ae2": "npm run build:ae2 && GIT_DEPLOY_DIR=web GIT_DEPLOY_BRANCH=gh-pages-ae2 GIT_DEPLOY_REPO=mv ./deploy.sh",
    "deploy:s": "npm run build:s && GIT_DEPLOY_DIR=web/dist GIT_DEPLOY_BRANCH=gh-pages-s GIT_DEPLOY_REPO=mv ./deploy.sh",
    "deploy:l": "npm run build:l && GIT_DEPLOY_DIR=web/dist GIT_DEPLOY_BRANCH=gh-pages-l GIT_DEPLOY_REPO=mv ./deploy.sh",
    "deploy:u": "npm run build:u && GIT_DEPLOY_DIR=web/dist GIT_DEPLOY_BRANCH=gh-pages-u GIT_DEPLOY_REPO=mv ./deploy.sh",
    "deploy:static": "npm run build:static && GIT_DEPLOY_DIR=web/dist GIT_DEPLOY_BRANCH=static GIT_DEPLOY_REPO=mv ./deploy.sh",
    "deploy:local": "npm run build:local && GIT_DEPLOY_DIR=web/dist GIT_DEPLOY_BRANCH=local GIT_DEPLOY_REPO=mv ./deploy.sh",
    "deploy:bots": "npm run build:bots && GIT_DEPLOY_DIR=web/dist GIT_DEPLOY_BRANCH=bots GIT_DEPLOY_REPO=mv ./deploy.sh",
    "lint": "eslint --quiet --ignore-path .gitignore .",
    "lintfile": "eslint --quiet --ignore-path .gitignore"
