{
  "name": "Tradle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node web/server",
    "dev": "NODE_ENV=development npm start",
    "prod": "NODE_ENV=production npm start",
    "postinstall": "npm run installhooks && ./web.sh",
    "installhooks": "cp hooks/* .git/hooks/",
    "clean": "rimraf web/dist/*",
    "copyassets": "shopt -s dotglob && cp -RL web/public/* web/dist && shopt -u dotglob",
    "analyze": "NODE_ENV=production node --max_old_space_size=4096 $(which webpack) --config web/webpack.config.js --json | node web/fix-analysis.js > web/dist/analysis.json && webpack-bundle-size-analyzer web/dist/analysis.json",
    "build:dev:raw": "mkdir -p web/dist && npm run clean && NODE_ENV=development node --max_old_space_size=4096 $(which webpack) --config web/webpack.config.js --progress --colors && npm run copyassets",
    "build": "./hooks/update_version.sh && mkdir -p web/dist && npm run clean && NODE_ENV=production node --max_old_space_size=4096 $(which webpack) --config web/webpack.config.js --progress --colors && npm run copyassets",
    "build:localstack": "PROVIDERS=localstack npm run build",
    "build:dev": "PROVIDERS=cloud npm run build:dev:raw",
    "build:prod": "PROVIDERS=cloud npm run build",
    "build:prod:image": "npm run build:prod && ./scripts/buildimage.sh cloud",
    "build:localstack:image": "npm run build:localstack && ./scripts/buildimage.sh localstack",
    "deploy:dev": "./scripts/deploy.sh deploy_dev",
    "deploy:promote": "./scripts/deploy.sh promote_dev",
    "rollback:dev": "./scripts/deploy.sh rollback_dev",
    "rollback:prod": "./scripts/deploy.sh rollback_prod",
    "docker:push": "docker push tradle/web-app",
    "lint": "eslint --quiet .",
    "precommit": "npm run lint",
    "prepush": "npm run lint",
    "update:models": "npm i -S tradle/models tradle/custom-models tradle/models-corporate-onboarding tradle/models-onfido tradle/models-products-bot && ./reshrink.sh",
    "translate:strings": "node ./scripts/translateStrings -d ./utils -l es",
    "translate:xmlstrings": "node ./scripts/translateStrings -d ./android/app/src/main/res/values -l vi,bn,fil,es,fr,nl,ru,km -f",
    "android-dev": "adb reverse tcp:8081 tcp:8081 && react-native run-android",
    "nibble": "eslint-nibble Components/ Store/ utils/ index.common.js"
  },
